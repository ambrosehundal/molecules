<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
<html>
    <body>
            <h1>Paired datasets testing</h1>

            <table class="table table-bordered table-striped table-hover" style="width:80%;margin-left:10%;">
                    <thead>
                      <tr>
                        <th scope="col">Dataset ID</th>  
                     

                        <th scope="col">Pairset A</th>
                        <th scope="col">Pairset B</th> 
                        <th scope="col">UCSC_CSC_Plate_ID</th> 
                        <th scope="col">Cell line</th> 
                        <th scope="col">Magnification</th> 
                        <th scope="col">TimePoint</th> 
                        <th scope="col">Experiment date</th> 

                      </tr>
                    </thead>
            
                    <tbody>
                    
                            <% for (var i = 0; i < testing.length; i++) { %>
                                <tr class="timeline"></tr>
                                <td class="timeline-yellow">
                                    <div class="timeline-time">
                                        <span style="text-align:left">
                                          <%= testing[i].dataset_id %>
                                        </span>
                                       
                                    </div>
                            </td>
                                <td class="timeline-yellow">
                                    
                                <a href= "/plates/<%= testing[i].pairset_A %>"><%= testing[i].pairset_A %></a>  
                                    
                                </td>   
                               
                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                  <a href= "/plates/<%= testing[i].pairset_B %>"><%= testing[i].pairset_B %></a>  
                                    </span>
                                   
                                </div>
                            </td>

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                  <a href= "/plates/<%= testing[i].pairset_B %>"><%= testing[i].UCSC_CSC_plate_ID %></a>  
                                    </span>
                                   
                                </div>
                            </td>

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                  <a href= "/plates/<%= testing[i].pairset_B %>"><%= testing[i].Cell_lines %></a>  
                                    </span>
                                   
                                </div>
                            </td>

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                  <a href= "/plates/<%= testing[i].pairset_B %>"><%= testing[i].Magnification %></a>  
                                    </span>
                                   
                                </div>
                            </td>

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                  <a href= "/plates/<%= testing[i].pairset_B %>"><%= testing[i].TimePoint %></a>  
                                    </span>
                                   
                                </div>
                            </td>

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                  <a href= "/plates/<%= testing[i].pairset_B %>"><%= testing[i].experiment_date %></a>  
                                    </span>
                                   
                                </div>
                            </td>
            
                            
            
                          
            
                        
            
                            
            
                            
            
                            
                        </tr>
                            <br>
                            <% } %>
                    
                      
                    </tbody>
                  </table>
            
    </body>
</html>
