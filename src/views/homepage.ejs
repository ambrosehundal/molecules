                            <h3>Search by plate</h3>
                                 <table class="table tpos table-bordered table-striped table-hover" id="platelist" style="width:50%; margin-top: 20%;">
                                         <thead>
                                           <tr>
                                             <th scope="col"> ID</th>
                                             <th scope="col">UCSC_CSC_plate_ID</th>
                                             <th scope="col">Alias name</th>
                                             
                                            
                                             
                                           </tr>
                                         </thead>
                                 
                                 
                                         <tbody>
                                         
                                                 <% for (var i = 0; i < print.length; i++) { %>
