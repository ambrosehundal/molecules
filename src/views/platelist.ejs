<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
<link rel="stylesheet" href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css">

<script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
<script src="https://code.jquery.com/jquery-3.3.1.js"></script>
<html>

        

        <body>

            <br>
            <br>
            <br>
            <br>
            <form action="/mycompound" method="GET">
                <label style="text-align:center; margin-left: 20%;font-size:24px;">
                    Search for a compound
                </label>
                <br>
                    <input type="text" name="compound" required style="text-align:center; margin-left: 20%;" />
                    <input type="submit" value="Submit" />
               
                
            </form>
          
            <br>


                <h1 style="text-align:center;font-size:36px;color:red"> Screening Center Platemap list</h1>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
                <br>
    
                <table class="table tpos table-bordered table-striped table-hover" id="platelist" style="width:50%;margin-left:25%;">
                        <thead>
                          <tr>
                            <th scope="col"> ID</th>
                            <th scope="col">UCSC_CSC_plate_ID</th>
                            <th scope="col">Alias name</th>
                            
                           
                            
                          </tr>
                        </thead>
                
                
                        <tbody>
                        
                                <% for (var i = 0; i < print.length; i++) { %>
                                    <tr class="timeline"></tr>
                                    <td><%= i + 1%></td>
                                    <td class="timeline-yellow">
                                        <div class="timeline-time">
                                            <span style="text-align:left">
                                          <a href= "/platelist/<%= i+1 %> "><%= print[i].UCSC_CSC_plate_ID %></a>  
                                            </span>
                                           
                                        </div>
                                    </td>
                                    <td class="timeline-yellow">
                                        <div class="timeline-time">
                                            <span style="text-align:left">
                                                <a href= "/platelist/<%= i+1 %> "><%= print[i].alias_name %></a>  
                                            </span>
                                           
                                        </div>
                                    </td>
                               
                            </tr>
                                <br>
                                <% } %>
                        
                          
                        </tbody>
                      </table>


               <br>
               <br>

            

               
                    
                    
                
                    
        
        </body>
        
        
        

</html>

<style>
    .tpos {
  
    position: absolute;
    top: 15%;
}

body{

}

a{
    text-decoration: none;
    color: black;
}
</style>



<script>
    $(document).ready(function() {
    $('#platelist').DataTable();

    document.getElementById('opacity-slider').addEventListener('input', function (e) {
	document.body.style.opacity = this.value;
});
} 

);




</script>


