<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
<html>
        

        <body>
                <h1 style="text-align:center">Platelist by unique name</h1>
                <br>
                <br>
                <table class="table tpos" style="width:50%;margin-left:10%;">
                        <thead>
                          <tr>
                            <th scope="col"> ID</th>
                            <th scope="col">UCSC_CSC_plate_ID</th>
                            <th scope="col">Unique Plate ID</th>
                            
                           
                            
                          </tr>
                        </thead>
                
                
                        <tbody>
                        
                                <% for (var i = 0; i < print.length; i++) { %>
                                    <tr class="timeline"></tr>
                                    <td><%= i + 1%></td>
                                    <td class="timeline-yellow">
                                        <div class="timeline-time">
                                            <span style="text-align:left">
                                          <a href= "/platelist/<%= i+1 %> "><%= print[i].UCSC_CSC_plate_ID %></a>  
                                            </span>
                                           
                                        </div>
                                    </td>
                                    <td class="timeline-yellow">
                                        <div class="timeline-time">
                                            <span style="text-align:left">
                                                    <%= print[i].unique_plate_id%> 
                                            </span>
                                           
                                        </div>
                                    </td>
                               
                            </tr>
                                <br>
                                <% } %>
                        
                          
                        </tbody>
                      </table>
               <br>
               <br>

               
                    
                    
                
                    
        
        </body>
        
        
        

</html>

<style>
    .tpos {
  
    position: absolute;
    top: 15%;
}

body{

}

a{
    text-decoration: none;
    color: black;
}
</style>






