            <% for (var i = 0; i < 1; i++) { %>
            <h1 style="text-align:center;font-size:40px;"> <%= print[i].UCSC_CSC_plate_ID %> experiments</h1>
            <% } %>
            <table class="table table-bordered table-striped table-hover" style="width:80%;margin-left:10%;  box-shadow: 0 19px 38px rgba(0,0,0,0.30), 0 15px 12px rgba(0,0,0,0.22);">
                    <thead style="background-color: #144878">
                      <tr style="color:white">
              
                        <th scope="col">Dataset ID</th>  
                       
                        <th scope="col">UCSC_CSC_Plate_ID</th> 
                        <th scope="col">Cell line</th> 
                        <th scope="col">Magnification</th> 
                        <th scope="col">TimePoint</th> 
                        <th scope="col">Experiment date</th> 

                      </tr>
                    </thead>
            
                    <tbody>
                    
                            <% for (var i = 0; i < print.length; i++) { %>
                                <tr class="timeline"></tr>
                               
                                
                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                  <a href= "/dataset/<%= print[i].plate_pair_id %>"><%= print[i].plate_pair_id %></a>  
                                    </span>
                                   
                                </div>
                            </td>

                            

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                  <%= print[i].UCSC_CSC_plate_ID %>
                                    </span>
                                   
                                </div>
                            </td>

                        

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                 <%= print[i].Cell_lines %>
                                    </span>
                                   
                                </div>
                            </td>

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                <%= print[i].Magnification %>
                                    </span>
                                   
                                </div>
                            </td>

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                 <%= print[i].TimePoint %>  
                                    </span>
                                   
                                </div>
                            </td>

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                 <%= print[i].experiment_date %>  
                                    </span>
                                   
                                </div>
                            </td>
                            
                        </tr>
                            <br>
                            <% } %>
                    
                      
                    </tbody>
                  </table>
            
