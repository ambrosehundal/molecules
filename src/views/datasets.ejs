<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
<html>
    <body>
        <br>
        <br>
        
            <% for (var i = 0; i < 1; i++) { %>
            <h1 style="text-align:center;font-size:40px;"> <%= print[i].UCSC_CSC_plate_ID %> plate datasets</h1>
            <% } %>
            <table class="table table-bordered table-striped table-hover" style="width:80%;margin-left:10%;">
                    <thead>
                      <tr>
              
                        <th scope="col">Dataset ID</th>  
                       
                        <th scope="col">UCSC_CSC_Plate_ID</th> 
                        <th scope="col">Cell line</th> 
                        <th scope="col">Magnification</th> 
                        <th scope="col">TimePoint</th> 
                        <th scope="col">Experiment date</th> 

                      </tr>
                    </thead>
            
                    <tbody>
                    
                            <% for (var i = 0; i < print.length; i++) { %>
                                <tr class="timeline"></tr>
                               
                                
                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                  <a href= "/platelist/<%= print[i].dataset_id %>"><%= print[i].dataset_id %></a>  
                                    </span>
                                   
                                </div>
                            </td>

                            

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                  <%= print[i].UCSC_CSC_plate_ID %>
                                    </span>
                                   
                                </div>
                            </td>

                        

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                 <%= print[i].Cell_lines %>
                                    </span>
                                   
                                </div>
                            </td>

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                <%= print[i].Magnification %>
                                    </span>
                                   
                                </div>
                            </td>

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                 <%= print[i].TimePoint %>  
                                    </span>
                                   
                                </div>
                            </td>

                            <td class="timeline-yellow">
                                <div class="timeline-time">
                                    <span style="text-align:left">
                                 <%= print[i].experiment_date %>  
                                    </span>
                                   
                                </div>
                            </td>
                            
                        </tr>
                            <br>
                            <% } %>
                    
                      
                    </tbody>
                  </table>
            
    </body>
</html>
