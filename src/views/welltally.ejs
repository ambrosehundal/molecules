<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

<h1>List of all wells for a dataset</h1>
<br>
<p>Wells are associated with each dataset
</p>

<table class="table" style="width:80%;margin-left:10%;">
        <thead>
          <tr>
            <th scope="col">ID</th>
            <th scope="col">Well name</th>
            <th scope="col">Cell well ID/Dataset ID</th>

            
            
            
          </tr>
        </thead>


        <tbody>
        
                <% for (var i = 0; i < wells.length; i++) { %>
                    <tr class="timeline"></tr>
                    <td class="timeline-yellow">
                        
                    <a href= "/plate"><%= wells[i].single_well_id %></a>  
                        
                    </td>   
                   
                <td class="timeline-yellow">
                    <div class="timeline-time">
                        <span style="text-align:left">
                      <a href= ""><%= wells[i].well_name %></a>  
                        </span>
                       
                    </div>
                </td>

                <td class="timeline-yellow">
                        <div class="timeline-time">
                            <span style="text-align:left">
                                    <%= wells[i].cell_well_id %> 
                            </span>
                           
                        </div>
                </td>

            

                

                

                
            </tr>
                <br>
                <% } %>
        
          
        </tbody>
      </table>
