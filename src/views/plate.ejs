<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

<h1>Scroll down to see data</h1>

<table class="table" style="width:50%;margin-left:10%;">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Plate ID</th>
            <th scope="col">384 Plate ID</th>
            
          </tr>
        </thead>


        <tbody>
        
                <% for (var i = 0; i < print.length; i++) { %>
                    <tr class="timeline"></tr>
                    <td><%= i + 1 %></td>
                <td class="timeline-yellow">
                    <div class="timeline-time">
                        <span style="text-align:left">
                      <a href= "/plate/:id"><%= print[i].UCSC_CSC_plate_ID %></a>  
                        </span>
                       
                    </div>
                </td>

                <td class="timeline-yellow">
                        <div class="timeline-time">
                            <span style="text-align:left">
                                    <%= print[i].plate_ID_384 %> 
                            </span>
                           
                        </div>
                </td>
            </tr>
                <br>
                <% } %>
        
          
        </tbody>
      </table>

