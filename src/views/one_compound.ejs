<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

<h1>Molecule results</h1>

<table class="table tpos table-bordered table-striped table-hover" style="width:80%;margin-left:10%;">
        <thead>
          <tr>
            <th scope="col">ID</th>
            <th scope="col">Plate ID</th>
            <th scope="col">Well Number</th>
            <th scope="col">Molecule name</th>
            <th scope="col">Concentration</th>
            

            
            
          </tr>
        </thead>


        <tbody>
        
                <% for (var i = 0; i < well.length; i++) { %>
                    <tr class="timeline"></tr>
                    <td><%= i+ 1%></td>
                <td class="timeline-yellow">
                    <div class="timeline-time">
                        <span style="text-align:left">
                      <a href= "/plates/<%= i + 1 %>"><%= well[i].UCSC_CSC_plate_ID %></a>  
                        </span>
                       
                    </div>
                </td>

                <td class="timeline-yellow">
                        <div class="timeline-time">
                            <span style="text-align:left">
                                    <%= well[i].Well %> 
                            </span>
                           
                        </div>
                </td>


                <td class="timeline-yellow">
                    <div class="timeline-time">
                        <span style="text-align:left">
                                <%= well[i].molecule_name %> 
                        </span>
                       
                    </div>
                </td>

                <td class="timeline-yellow">
                    <div class="timeline-time">
                        <span style="text-align:left">
                                <%= well[i].Concentration %> 
                        </span>
                       
                    </div>
                </td>


               

                
            </tr>
                <br>
                <% } %>
        
          
        </tbody>
      </table>

