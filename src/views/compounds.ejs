<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

<head>
    <nav class="navbar navbar-expand-lg navbar-light bg-light" style="background-color: #070999 !important;">
            <a class="navbar-brand" href="/" style="color:white !important;">CP Image Viewer</a>
    </nav>

</head>


<h1>Search Results</h1>
<br>

<table class="table tpos table-bordered table-striped table-hover" style="width:80%;margin-left:10%;">
        <thead style="background-color: rgb(9, 39, 206)">
          <tr style="color:white;">
            <th scope="col">ID</th>
            <th scope="col">Plate ID</th>
            <th scope="col">Well Number</th>
            <th scope="col">Molecule name</th>
            <th scope="col">Concentration</th>
            

            
            
          </tr>
        </thead>


        <tbody>
             <tr>
                <% for (var i = 0; i < well.length; i++) { %>
                    <td><%= i+ 1%></td>
                <td>
                    <div>
                        <span style="text-align:left">
                      <a href= "/plates/<%= i + 1 %>"><%= well[i].UCSC_CSC_plate_ID %></a>  
                        </span>
                       
                    </div>
                </td>

                <td>
                        <div>
                            <span style="text-align:left">
                                    <%= well[i].Well %> 
                            </span>
                           
                        </div>
                </td>


                <td>
                    <div>
                        <span style="text-align:left">
                                <%= well[i].molecule_name %> 
                        </span>
                       
                    </div>
                </td>

                <td>
                    <div>
                        <span style="text-align:left">
                                <%= well[i].Concentration %> 
                        </span>
                       
                    </div>
                </td>


               

                
            </tr> 
            
                 <% } %> 
        
          
        </tbody>
      </table>

