<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
<link rel="stylesheet" href="//cdn.datatables.net/1.10.20/css/jquery.dataTables.min.css">
<link href="https://fonts.googleapis.com/css?family=Montserrat:500&display=swap" rel="stylesheet">
<html>

<script src="https://code.jquery.com/jquery-3.3.1.js"></script>
<head>
    <nav class="navbar navbar-expand-lg navbar-light bg-light" style="background-color: #070999 !important;">
            <a class="navbar-brand" href="/" style="color:white !important;">CP Image Viewer</a>
    </nav>

</head>


<h1>Search Results for</h1>
<br>

<body>

<div class="container">
    <div class="col-md-12">

            <table class="table tpos table-bordered table-striped table-hover" id="compounds">
                    <thead style="background-color: rgb(9, 39, 206)">
                      <tr style="color:white;">
                        <th scope="col">ID</th>
                        <th scope="col">Plate ID</th>
                        <th scope="col">Well Number</th>
                        <th scope="col">Molecule name</th>
                        <th scope="col">Concentration</th>
                        
            
                        
                        
                      </tr>
                    </thead>
            
            
                    <tbody>
                         <tr>
                            <% for (var i = 0; i < well.length; i++) { %>
                                <td><%= i+ 1%></td>
                            <td>
                                <div>
                                    <span style="text-align:left">
                                  <a href= "/platelist/<%= i + 1 %>"><%= well[i].UCSC_CSC_plate_ID %></a>  
                                    </span>
                                   
                                </div>
                            </td>
            
                            <td>
                                    <div>
                                        <span style="text-align:left">
                                                <%= well[i].Well %> 
                                        </span>
                                       
                                    </div>
                            </td>
            
            
                            <td>
                                <div>
                                    <span style="text-align:left">
                                            <%= well[i].molecule_name %> 
                                    </span>
                                   
                                </div>
                            </td>
            
                            <td>
                                <div>
                                    <span style="text-align:left">
                                            <%= well[i].Concentration %> 
                                    </span>
                                   
                                </div>
                            </td>
            
            
                           
            
                            
                        </tr> 
                        
                             <% } %> 
                    
                      
                    </tbody>
                  </table>

    </div>
</div>



    </body>

      <script src="//cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>

      <script>
        $(document).ready( function () {
        $('#compounds').DataTable({
            // List of options to display results
        "lengthMenu": [[10, 25, 50, -1], [10, 25, 50, "All"]]
    
        }
    
        );
    
       
    } );
    </script>


</html>


<style>
      body{
    font-family: 'Montserrat', sans-serif;

  }  
</style>