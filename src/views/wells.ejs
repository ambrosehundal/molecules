<div class="container">
    <div class="row">
        <div class="col-md-12">
                <div class="col-md-6">
                        <h1 style="text-align: center;">Stain Set 1</h1>
                        <br>

                        <!-- WAVELENGTH_1 IMAGES -->
                        <div class="row">

                                <% for (var i = 0; i < well[0].length ; i++) { %>
                           
                           
                               
                                    <img onmouseover="enlarge(this)" id="well_image" onmouseout="normalImage(this)" src="/4664/<%= well[0][i].w1_filepath %>" height="250" width="250" style="display:inline-block;"/>  
                            
                    

                        

                        <% } %>

                        </div>

                        <br>

                        <!-- WAVELENGTH_2 IMAGES -->
                        <div class="row">
                                <% for (var i = 0; i < well[1].length ; i++) { %>
                           
                           
                               
                                    <img onmouseover="enlarge(this)" onmouseout="normalImage(this)" src="/4664/<%= well[1][i].w2_filepath %>" height="250" width="250" style="display:inline-block;"/>  
                            
                    
        
                       
        
                        <% } %>
                            
                        </div>

                        <br>
                    

                        <!-- WAVELENGTH_3 IMAGES -->
                        <div class="row">
                                <% for (var i = 0; i < well[2].length ; i++) { %>
                           
                           
                               
                                    <img onmouseover="enlarge(this)" onmouseout="normalImage(this)" src="/4664/<%= well[2][i].w3_filepath %>" height="250" width="250" style="display:inline-block;"/>  
                            
                    
                
                 
                
                        <% } %>
                            
                        </div>

                        <br>

                        <!-- WAVELENGTH_4 IMAGES -->
                        <div class="row">
                                <% for (var i = 0; i < well[3].length ; i++) { %>
                           
                           
                               
                                    <img onmouseover="enlarge(this)" onmouseout="normalImage(this)" src="/4664/<%= well[3][i].w4_filepath %>" height="250" width="250" style="display:inline-block;"/>  
                            
                        
                        
                    
                        
                        <% } %>
                            
                        </div>


                                

    
         
            </div> 

            <div class="col-md-6">
                    <h1 style="text-align: center;">Stain Set 2</h1>
                    <br>

                    <!-- WAVELENGTH_1 IMAGES -->
                    <div class="row">

                            <% for (var i = 0; i < well[0].length ; i++) { %>
                       
                       
                           
                                <img onmouseover="enlarge(this)" onmouseout="normalImage(this)" src="/4664/<%= well[0][i].w1_filepath %>" height="250" width="250" style="display:inline-block;"/>  
                        
                

                    

                    <% } %>

                    </div>

                    <br>

                    <!-- WAVELENGTH_2 IMAGES -->
                    <div class="row">
                            <% for (var i = 0; i < well[1].length ; i++) { %>
                       
                       
                           
                                <img onmouseover="enlarge(this)" onmouseout="normalImage(this)" src="/4664/<%= well[1][i].w2_filepath %>" height="250" width="250" style="display:inline-block;"/>  
                        
                
    
                   
    
                    <% } %>
                        
                    </div>

                    <br>
                

                    <!-- WAVELENGTH_3 IMAGES -->
                    <div class="row">
                            <% for (var i = 0; i < well[2].length ; i++) { %>
                       
                       
                           
                                <img onmouseover="enlarge(this)" onmouseout="normalImage(this)" src="/4664/<%= well[2][i].w3_filepath %>" height="250" width="250" style="display:inline-block;"/>  
                        
                
            
             
            
                    <% } %>
                        
                    </div>

